{
    "version": 2,
    "builds": [
        {
            "src": "*.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "functions": {
        "api/*": {
            "memory": 512,
            "maxBodySize": "50mb"
        }
    }
}
